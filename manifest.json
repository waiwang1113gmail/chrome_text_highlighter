{
  "manifest_version": 2,

  "name": "Chrome Highlighter Extension",
  "description": "This extension highlights matched text on current pages",
  "version": "1.0",

  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*","http://*/*"],
      //"css": ["mystyles.css"],
      "js": ["bower_components/jquery/dist/jquery.js", "content.js"]
    }
  ]
}